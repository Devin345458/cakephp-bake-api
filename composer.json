{
    "name": "devin345458/cakephp-bake-api",
    "description": "Api Bake Theme plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Api\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Api\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
